#include <bits/stdc++.h>
using namespace std;

int main(){
  int n = 4543;
   int sum = 0;
  cout<<"kuch to aa"<<endl;
  while(n > 0 || sum > 9){
     if(n == 0) {
         n = sum;
           sum = 0; }

    sum = sum + n % 10;
    n = n/10;
    
  }
  cout<<"sum of the value :"<<sum<<endl;
  
 return 0; 
}
