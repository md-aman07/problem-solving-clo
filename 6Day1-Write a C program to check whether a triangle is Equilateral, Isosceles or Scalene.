#include <stdio.h>
int main(void) {
   int a,b,c;
  printf("enter the values");
  scanf("%d %d %d",&a,&b,&c);

   if (a==b && b==c && c==a)
   {
     printf("equilateral");
   }
  else if (a!=b && b!=c && c!=a)
  {  printf ("scalene"); 
    }
   else
  {
    printf("isosceles");
  }
   
  return 0;
}
